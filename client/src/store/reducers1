// This file contains reducers for managing application state. 

import { combineReducers } from 'redux';
import authReducer from './actions/authReducer';
import quizReducer from './actions/quizReducer';

// Combine all reducers into a single reducer
const rootReducer = combineReducers({
  auth: authReducer,
  quiz: quizReducer,
});

export default rootReducer;